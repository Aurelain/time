{
    "name": "time",
    "version": "0.1.0",
    "homepage": "./",
    "private": true,
    "dependencies": {
        "@emotion/react": "^11.7.1",
        "@emotion/styled": "^11.6.0",
        "@fontsource/roboto": "^4.5.1",
        "@mui/material": "^5.2.8",
        "assert": "^2.0.0",
        "copy-to-clipboard": "^3.3.1",
        "mdi-material-ui": "^7.1.0",
        "memoize-one": "^6.0.0",
        "open": "^8.4.0",
        "prop-types": "^15.8.1",
        "prop-types-exact": "^1.2.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.6",
        "react-scripts": "5.0.0",
        "redux": "^4.1.2",
        "redux-devtools-extension": "^2.13.9",
        "redux-thunk": "^2.4.1",
        "reselect": "^4.1.5"
    },
    "scripts": {
        "start": "node scripts/start",
        "focus": "node scripts/focus",
        "build": "node scripts/build"
    },
    "eslintConfig": {
        "extends": "react-app",
        "parserOptions": {
            "ecmaVersion": 2021
        },
        "rules": {
            "max-len": [
                "warn",
                {
                    "code": 120
                }
            ],
            "quotes": [
                "warn",
                "single",
                "avoid-escape"
            ]
        }
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true,
        "printWidth": 120,
        "bracketSpacing": false
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "inquirer": "^8.2.0",
        "mini-css-extract-plugin": "2.4.5",
        "prettier": "^2.5.1"
    }
}
